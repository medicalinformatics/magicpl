<?xml version="1.0" encoding="UTF-8"?>
<paths xmlns="http://www.example.org/Pfade" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/Pfade paths.xsd ">
	<path>
		<name>toLower</name>
		<parameters></parameters>
		<implementation>de.mainzelliste.paths.processor.LowerCase</implementation>
	</path>
	<path>
		<name>getCn</name>
		<parameters></parameters>
		<implementation>de.mainzelliste.paths.processor.CngClient</implementation>
	</path>
	<path>
		<name>getTkt</name>
		<parameters></parameters>
		<implementation>de.mainzelliste.paths.processor.MainzellisteClient</implementation>
	</path>
	<multipath>
		<name>getCnTkt</name>
		<path>
			<name>getCn</name>
			<parameters></parameters>
			<implementation>de.mainzelliste.paths.processor.CngClient</implementation>
		</path>
		<path>
			<name>getTkt</name>
			<parameters></parameters>
			<implementation>de.mainzelliste.paths.processor.MainzellisteClient</implementation>
		</path>
	</multipath>

	<path>
		<name>selectorPath</name>
		<parameters></parameters>
		<switch evaluator="evaluatorClass">
			<case value="foo">
				<path>
					<name>fooPath</name>
					<implementation></implementation>
				</path>
			</case>
			<default>
				<multipath>
					<name>defaultMultiPath</name>
					<parameters></parameters>
					<path>
						<name>defaultMulti1</name>
						<implementation></implementation>
					</path>
					<path>
						<name>defaultMulti2</name>
						<implementation></implementation>
					</path>
				</multipath>
			</default>
		</switch>
	</path>
</paths>
